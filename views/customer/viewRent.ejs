<h2 class="text-info">ငှား ထားသောစာရင်း</h2>
<table class="table table-dark" style="margin-top: 20px">
    <thead>
      <tr>
  
        <th scope="col">ဇာတ်ကားအိုင်ဒီ</th>
        <th scope="col">ဇာတ်ကား နာမည်</th>
        <th scope="col">အရေအတွက်</th>
        <th scope="col">အပ်ရမဲ့ ရက်</th>
        <th scope="col">ငှားရမ်းသည့် ရက်</th>
        <th scope="col">စုစုပေါင်း ငှားခ</th>
        
        <th scope="col">နာမည်</th>
        <th scope="col">ဖုန်း</th>
        <th scope="col">လိပ်စာ</th>
        <th scope="col">ရက်လွန်ရက်</th>
        <th scope="col">ရက်လွန်ကြေး</th>
        <th scope="col">အပ်ပြီး</th>
      
        <!-- <th scope="col" colspan=""></th> -->
      </tr>
    </thead>
    <tbody>
      <% rents.forEach(function(rent) { 
        let today = new Date();
        let days;

        let overDueDay = 0;
        let overDueFee = 0;
        if(today > rent.dueDate && !rent.finished ){
          overDueDay = parseInt(Math.round((today-rent.dueDate) / 86400000));
          overDueFee = parseInt(overDueDay * 200);
        }
        
        %>
      <tr>
  
        <td><%= rent.movieId %></td>
        <td><%= rent.movieName %></td>
        <td><%= rent.qty %></td>
        <td><%= rent.dueDate.toLocaleDateString() %></td>
        <td><%= rent.rentedDate.toLocaleDateString() %></td>
        <td><%= rent.totalPrice %></td>
        <td><%= rent.name %></td>
        <td><%= rent.phNo %></td>
        <td><%= rent.address %></td>
        <td><%= overDueDay %></td>
        <td><%= overDueFee %></td>
        <td><%= rent.finished ? "yes" : "no" %></td>
       
     
      </tr>
      <% }) %>
    </tbody>
  </table>

<%- include('../partials/footer') %>
