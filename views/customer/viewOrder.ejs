<%- include('../partials/header') %>
<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <a class="navbar-brand" href="../<%=id%>">PresentVcd</a>

</nav>
<br />


<h2 class="text-info">ကြိုမှာထားသောခွေ</h2>
<table class="table table-dark" style="margin-top: 20px">
    <thead>
      <tr>
  
        <th scope="col">ဇာတ်ကားအိုင်ဒီ</th>     
        <th scope="col">ဇာတ်ကားနာမည်</th>     
        <th scope="col">စျေးနှူန်း</th>     
        
        <th scope="col">နာမည်</th>
        <th scope="col">ဖုန်း</th>
        <th scope="col">လိပ်စာ</th>
        <th scope="col">အရေအတွက်</th>
        <th scope="col">အော်ဒါမှာသည့်ရက်စွဲ</th>
      
      
        <!-- <th scope="col" colspan=""></th> -->
      </tr>
    </thead>
    <tbody>
      <% orders.forEach(function(order) { 
     
        
        %>
      <tr>
  
        <td><%= order.movieId %></td>
        <td><%= order.movieName %></td>
        <td><%= order.moviePrice %></td>
        <td><%= order.name %></td>
        <td><%= order.phNo %></td>
        <td><%= order.address %></td>
        <td><%= order.qty %></td>
        <td><%= order.orderDate.toLocaleDateString() %></td>
       
     
      </tr>
      <% }) %>
    </tbody>
  </table>

<%- include('../partials/footer') %>
